{
    "version": 2,
    "functions": {
        "api/**/*.js": {
            "runtime": "@vercel/node@3.0.11"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ]
}